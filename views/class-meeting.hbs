<div class="kt-container  kt-container--fluid  kt-grid__item kt-grid__item--fluid">
    <div class='row'>
        <div class="col-lg-12">

            <!--begin::Portlet-->
            <div class="kt-portlet" id="kt_portlet">
                <div class="kt-portlet__head">
                    <div class="kt-portlet__head-label">
                        <span class="kt-portlet__head-icon">
                            <i class="flaticon-calendar"></i>
                        </span>
                        <h3 class="kt-portlet__head-title">
                            List View
                        </h3>
                    </div>
                </div>
                <div class="kt-portlet__body">
                    <div id="kt_calendar" style="" class="fc fc-ltr fc-unthemed">
                        <!--begin::Calendar Toolbar-->
                        <div class="fc-toolbar fc-header-toolbar">
                            <div class="fc-left"></div>
                            <div class="fc-center">
                                <h2>INCOMING MEETINGS</h2>
                            </div>
                            <div class="fc-right"></div>
                        </div>
                        <!--end::Calendar Toolbar-->

                        <!--begin::List meeting-->
                        <div class="fc-view-container">
                            <div class="fc-view fc-listWeek-view fc-list-view fc-widget-content" style="">
                                <div class="fc-scroller" style="overflow: hidden auto; height: 748px;">
                                    <table class="fc-list-table ">
                                        <!--begin::Meeting container-->
                                        <tbody id="et-meeting-container">
                                        </tbody>
                                        <!--end::Meeting container-->
                                    </table>
                                </div>
                            </div>
                        </div>
                        <!--end::List meeting-->
                    </div>
                </div>
            </div>
            <!--end::Portlet-->

        </div>
    </div>

    <!--begin::Add new meeting modal-->
    <div class="modal fade" id="et_modal_add_new_meeting" tabindex="-1" role="dialog"
        aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Add new meeting</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    </button>
                </div>
                <div class="modal-body">
                    <!--begin::Form-->
                    <form class="kt-form kt-form--label-right">
                        <div class="kt-portlet__body">
                            <div class="form-group row">
                                <div class="col-lg-6">
                                    <label>Meeting name:</label>
                                    <input type="text" class="form-control" placeholder="Enter meeting name"
                                        id="et-meeting-name-input">
                                    <span class="form-text text-muted">Please enter the name of this meeting</span>
                                </div>
                                <div class="col-lg-6">
                                    <label>Duration (hours):</label>
                                    <input type="number" class="form-control" placeholder="Enter duration" min=0
                                        id="et-meeting-duration-input">
                                    <span class="form-text text-muted">Please enter duration of this meeting</span>
                                </div>
                            </div>
                            <div class="form-group row">
                                <div class="col-lg-6">
                                    <label>Start time:</label>
                                    <div class="input-group date">
                                        <input type="text" class="form-control" readonly
                                            placeholder="Select date & time" id="kt_datetimepicker_2" />
                                        <div class="input-group-append">
                                            <span class="input-group-text"><i
                                                    class="la la-calendar-check-o glyphicon-th"></i></span>
                                        </div>
                                    </div>
                                    <span class="form-text text-muted">Please enter the start time of this
                                        meeting</span>
                                </div>
                            </div>
                        </div>
                    </form>

                    <!--end::Form-->
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary" id="et-add-new-meeting-btn">Add Meeting</button>
                </div>
            </div>
        </div>
    </div>
    <!--end::Add new meeting modal-->
</div>